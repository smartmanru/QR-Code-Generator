{
  "name": "qr-code-generator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@mdi/js": "^7.1.96",
    "@smui/card": "^6.1.4",
    "@smui/checkbox": "^6.1.4",
    "@smui/icon-button": "^6.1.4",
    "@smui/layout-grid": "^6.1.4",
    "@smui/paper": "^6.1.4",
    "@smui/select": "^6.2.0",
    "@smui/slider": "^6.1.4",
    "@smui/tab": "^6.1.4",
    "@smui/tab-bar": "^6.1.4",
    "@smui/textfield": "^6.1.4",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "awesome-qr": "^2.1.5-rc.0",
    "qrcode-svg": "^1.1.0",
    "smui-theme": "^6.2.0",
    "svelte": "^3.55.1",
    "svelte-material-ui": "^6.2.0",
    "vite": "^4.1.4"
  }
}
